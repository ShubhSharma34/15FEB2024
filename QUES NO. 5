//QUES 5 Code to input a string containg multiple lines using scanf
#include<stdio.h>
#include<string.h>
int main()
{
char ss[1000];
printf("Enter the multiple lines(press Ctrl+D on a newline to stop)\n");
while(scanf("%[^\n}%*c",ss)==1){
printf("%s\n"ss);   //DEMONSTRATE THE SENCTANCES
}
return 0;
}
